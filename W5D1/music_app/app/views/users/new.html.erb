<div class="login">
<form action="<%=users_url%>" method="post">
  <%= auth_token %>
  <label for="email">
    Email:
    <input type="text" name="user[email]" value="<%=@user.email%>">
  </label>
  <br>
  <label for="password">
    Password:
    <input type="password" name="user[password]">
  </label>
  <br>

  <input type="submit" value="Create User!">
</form>



<div class = "login">
<form action="<%=session_url%>" method="post">
  <%= auth_token %>
  <label for="email">
    Email:
    <input type="text" name="user[email]">
  </label>
  <br>
  <label for="password">
    Password:
    <input type="password" name="user[password]">
  </label>
  <br>

  <input type="submit" value="Log In User!">
</form>
</div>
